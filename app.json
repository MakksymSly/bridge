{
	"expo": {
		"doctor": {
			"reactNativeDirectoryCheck": {
				"exclude": ["react-native-bouncy-checkbox", "i18next"]
			}
		},
		"name": "bridge-list",
		"slug": "bridge-list",
		"version": "1.0.1",

		"orientation": "portrait",
		"icon": "./assets/images/logo.png",
		"scheme": "myapp",
		"userInterfaceStyle": "automatic",
		"newArchEnabled": true,
		"ios": {
			"supportsTablet": true
		},
		"android": {
			"versionCode": 2,
			"softwareKeyboardLayoutMode": "pan",
			"adaptiveIcon": {
				"foregroundImage": "./assets/images/adaptive-icon.png",
				"backgroundColor": "#ffffff"
			},
			"package": "com.maksymsly.bridgelist"
		},
		"web": {
			"bundler": "metro",
			"output": "static",
			"favicon": "./assets/images/adaptive-icon.png"
		},
		"plugins": [
			"expo-router",
			[
				"expo-splash-screen",
				{
					"image": "./assets/images/adaptive-icon.png",
					"imageWidth": 200,
					"resizeMode": "contain",
					"backgroundColor": "#ffffff"
				}
			],
			"expo-localization"
		],
		"experiments": {
			"typedRoutes": true
		},
		"extra": {
			"router": {
				"origin": false
			},
			"eas": {
				"projectId": "c9b2c4bd-0a8f-4a74-b02d-6cbab3806b8a"
			}
		}
	}
}
